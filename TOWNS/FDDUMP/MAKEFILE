ALL : EXE\FDDUMP.EXE EXE\RDD2D77.EXE EXE\TEST.EXE

# BUILD:
#	MD BUILD

# EXE:
#	MD EXE

# Requires Watcom C


BUILD\FDDUMP.OBJ : FDDUMP.C MAKEFILE
	WCC FDDUMP.C -3 -foBUILD\FDDUMP.OBJ

EXE\FDDUMP.EXE : BUILD\FDDUMP.OBJ MAKEFILE
	@WLINK FILE BUILD\FDDUMP.OBJ NAME EXE\FDDUMP.EXE



BUILD\RDD2D77.OBJ : RDD2D77.C MAKEFILE
	WCC RDD2D77.C -3 -foBUILD\RDD2D77.OBJ

EXE\RDD2D77.EXE : BUILD\RDD2D77.OBJ MAKEFILE
	@WLINK FILE BUILD\RDD2D77.OBJ NAME EXE\RDD2D77.EXE



BUILD\TEST.OBJ : TEST.C MAKEFILE
	WCC TEST.C -3 -foBUILD\TEST.OBJ

EXE\TEST.EXE : BUILD\TEST.OBJ MAKEFILE
	@WLINK FILE BUILD\TEST.OBJ NAME EXE\TEST.EXE
