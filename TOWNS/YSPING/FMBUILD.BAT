REM Build for FM-R / FM-TOWNS BIOS


IMGMOUNT A F:\EMU\TOWNS\TRANSFER.XDF -t floppy


DEL ERR.TXT




DEL UT_INT77.EXE

MASM UT_INT77.ASM UT_INT77.OBJ UT_INT77.LST NUL >> ERR.TXT
LINK UT_INT77;


DEL 386INT77.EXP
DEL 386INT77.MAP

386ASM 386INT77.ASM >> ERR.TXT
386LINK 386INT77.OBJ -STACK 4096 >> ERR.TXT



DEL YSPING.SYS

MASM YSPING.ASM YSPING.OBJ YSPING.LST NUL >> ERR.TXT
LINK YSPING; >> ERR.TXT
EXE2BIN YSPING.EXE YSPING.SYS >> ERR.TXT

DEL YSPING.EXE

DEL *.OBJ
DEL *.LST


COPY *.EXP A:\
COPY *.EXE A:\
COPY YSPING.SYS A:\


:EXIT
