ALL : BUILD EXE SYS SYS\YSSCSICD.SYS


BUILD :
	MKDIR BUILD

EXE :
	MKDIR EXE

SYS :
	MKDIR SYS



SYS\YSSCSICD.SYS : YSSCSICD.ASM PRINT.ASM RS232C.ASM FMR_TIMR.ASM HOOKINST.ASM HOOKBIOS.ASM YS_EXT.ASM REDRFUNC.ASM SCSIUTIL.ASM FMR_SCSI.ASM WRHIGH.ASM DATABUF.ASM ..\SCSILIB\SCSIIO.ASM
	MASM YSSCSICD.ASM BUILD\YSSCSICD.OBJ BUILD\YSSCSICD.LST NUL
	LINK BUILD\YSSCSICD.OBJ;
	EXE2BIN YSSCSICD.EXE SYS\YSSCSICD.SYS
	DEL YSSCSICD.EXE
	IMGMOUNT R: TESTFD\TESTFD.BIN -size 720
	COPY SYS\YSSCSICD.SYS R:
	MOUNT -u R:
