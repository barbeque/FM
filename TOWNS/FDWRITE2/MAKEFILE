FDWRITE2.EXP : FDWRITE2.OBJ DISKIMG.OBJ DMABUF.OBJ UTIL.OBJ ASM.OBJ MEM.OBJ Makefile
	HC386 -fsoft FDWRITE2.OBJ DISKIMG.OBJ DMABUF.OBJ UTIL.OBJ ASM.OBJ MEM.OBJ

FDWRITE2.OBJ : FDWRITE2.C
	hc386 -c FDWRITE2.C

DISKIMG.OBJ : DISKIMG.C
	hc386 -c DISKIMG.C

DMABUF.OBJ : DMABUF.C
	hc386 -c DMABUF.C

UTIL.OBJ : UTIL.C
	hc386 -c UTIL.C

ASM.OBJ : ASM.ASM
	386asm ASM.ASM

MEM.OBJ : MEM.ASM
	386asm MEM.ASM
